{
    "structSchema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "dynamic": "true",
        "datetime_detection": true,
        "geolocation_detection": true,
        "properties": {
            "airline": {
                "type": "string",
                "retrievable": true,
                "indexable": true,
                "dynamicFacetable": true,
                "searchable": true
            },
            "arrival_airport": {
                "type": "string",
                "retrievable": true,
                "indexable": true,
                "dynamicFacetable": true,
                "searchable": true
            },
            "arrival_gate": {
                "type": "string",
                "retrievable": true,
                "indexable": true,
                "dynamicFacetable": true,
                "searchable": true
            },
            "arrival_time": {
                "type": "datetime",
                "retrievable": true,
                "indexable": true
            },
            "departure_airport": {
                "type": "string",
                "retrievable": true,
                "indexable": true,
                "dynamicFacetable": true,
                "searchable": true
            },
            "departure_gate": {
                "type": "string",
                "retrievable": true,
                "indexable": true,
                "dynamicFacetable": true,
                "searchable": true
            },
            "departure_time": {
                "type": "datetime",
                "retrievable": true,
                "indexable": true
            },
            "flight_number": {
                "type": "string",
                "keyPropertyMapping": "title",
                "retrievable": true
            },
            "id": {
                "type": "number"
            }
        }
    }
}