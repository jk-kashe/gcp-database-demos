{
    "structSchema": {
        "$schema": "https://json-schema.org/draft/2020-12/schema",
        "type": "object",
        "dynamic": "true",
        "datetime_detection": true,
        "geolocation_detection": true,
        "properties": {
            "content": {
                "type": "string",
                "keyPropertyMapping": "description",
                "retrievable": true
            },
            "embedding": {
                "type": "string"
            },
            "id": {
                "type": "number"
            }
        }
    }
}